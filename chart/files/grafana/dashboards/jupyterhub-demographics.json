        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  COALESCE(u.department, 'Unknown') AS \"Department\",\n  COUNT(DISTINCT co.user_email) AS \"User Count\",\n  ROUND(SUM(CASE WHEN co.node_name NOT ILIKE '%cpu%' THEN runtime_hours ELSE 0 END)::numeric, 2) AS \"GPU Hours\",\n  ROUND(SUM(CASE WHEN co.node_name ILIKE '%cpu%' THEN runtime_hours ELSE 0 END)::numeric, 2) AS \"CPU Hours\",\n  ROUND(SUM(runtime_hours)::numeric, 2) AS \"Total Hours\"\nFROM (\n  SELECT \n    us.user_email,\n    us.node_name,\n    us.runtime_hours\n  FROM user_sessions us\n  WHERE us.session_start <= $__timeTo()\n    AND us.session_end >= $__timeFrom()\n) co\nLEFT JOIN users u ON co.user_email = u.email\nGROUP BY u.department\nORDER BY \"Total Hours\" DESC;",
          "refId": "A",
          "select": [
            [
              {
                "params": ["value"],
                "type": "column"
              }
            ]
          ],
          "timeColumn": "time",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
